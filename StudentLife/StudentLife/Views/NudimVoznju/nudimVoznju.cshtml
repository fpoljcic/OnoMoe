    <!--==========================
      Intro Section
      ============================-->
    <section id="intro">
        <div class="intro-container wow fadeIn">
            <form asp-controller="NudimVoznju" asp-action="PonudiVoznju">
                <label for="date" class="formaNudim">
                    Datum</p>
                    <input name="date" data-role="datepicker" data-year="false">
                    <br>
                    <label for="time" class="formaNudim">
                        Vrijeme</p>
                        <input name="time" data-role="timepicker" data-seconds="false">
                        <br>
                        <label for="cars" class="formaNudim">
                            Broj sjedišta</p>
                            <select name="cars">
                                <option value="1">1</option>
                                <option value="2">2</option>
                                <option value="3">3</option>
                                <option value="4">4</option>
                                <option value="5">5</option>
                                <option value="6">6</option>
                            </select>
                            <input id="p1" name="pocetni" type="hidden"/>
                            <input id="p2" name="krajnji" type="hidden"/>

                            <br>
                            <button class="btn btn-primary btn-block" id="btnPonudi" type="submit" style="background: #4ee44e;color:white;outline: none;margin:0;">
                                <b>PONUDI VOŽNJU</b>
                            </button>
            </form>
        </div>


        <div class="formaRegister">
            <p id="mapaLabel">Unesi mjesto polaska i odredište na mapi</p>
            <div id='map' class='map'></div>
        </div>
    </section>

