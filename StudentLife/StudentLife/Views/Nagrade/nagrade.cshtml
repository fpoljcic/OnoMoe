<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>StudentLife</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">
    <link rel="stylesheet" href="https://api.tomtom.com/maps-sdk-for-web/cdn/5.x/5.42.0/maps/maps.css">
    <link rel="stylesheet" href="https://api.tomtom.com/maps-sdk-for-web/cdn/5.x/5.42.0/maps/css-styles/routing.css">
    <link rel="stylesheet" href="https://api.tomtom.com/maps-sdk-for-web/5.x/5.42.0//examples/pages/examples/assets/ui-library/index.css">
    <script src="https://api.tomtom.com/maps-sdk-for-web/cdn/5.x/5.42.0/maps/maps-web.min.js"></script>
    <script src="https://api.tomtom.com/maps-sdk-for-web/5.x/5.42.0//examples/pages/examples/assets/js/mobile-or-tablet.js"></script>
    <script src="https://api.tomtom.com/maps-sdk-for-web/5.x/5.42.0//examples/pages/examples/assets/js/formatters.js"></script>
    <script src="https://api.tomtom.com/maps-sdk-for-web/cdn/5.x/5.42.0/services/services-web.min.js"></script>
    <!-- Favicons -->
    <link href="~/img/favicon.png" rel="icon">
    <link href="~/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Raleway:300,400,500,700,800" rel="stylesheet">

    <!-- Bootstrap CSS File -->
    <link href="~/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Libraries CSS Files -->
    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/lib/venobox/venobox.css" rel="stylesheet">
    <link href="~/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Main Stylesheet File -->
    <link href="~/css/nagrade.css" rel="stylesheet">

    <!-- =======================================================
      Theme Name: TheEvent
      Theme URL: https://bootstrapmade.com/theevent-conference-event-bootstrap-template/
      Author: BootstrapMade.com
      License: https://bootstrapmade.com/license/
      ======================================================= -->
</head>

<body>

    <!--==========================
      Header
      ============================-->
    <header id="header">
        <div class="container">

            <div id="logo" class="pull-left">
                <!-- Uncomment below if you prefer to use a text logo -->
                <!-- <h1><a href="#main">C<span>o</span>nf</a></h1>-->
                <a href="#intro" class="scrollto"><img src="~/img/logo.png" alt="" title=""></a>
            </div>

            <nav id="nav-menu-container">
                <form asp-controller="Home" asp-action="Index">
                    <ul class="nav-menu">
                        <li><a href="@Url.Action("Index", "NudimTrazimPrevoz")">Početna</a></li>
                        <li><a href="@Url.Action("Index", "MojePonudjeneVoznje")">Moje ponuđene vožnje</a></li>
                        <li><a href="@Url.Action("Index", "MojeTrazeneVoznje")">Moje tražene vožnje</a></li>
                        <li><a href="@Url.Action("Index", "Nagrade")">Nagrade</a></li>
                        <li> </li>
                        <li><button id="btnOdjaviSe" type="submit" style="background: #f82249; border-radius: 50px; border: none; width:100px;color:white">Odjavi se</button></li>
                        <li></li>
                    </ul>
                </form>
            </nav><!-- #nav-menu-container -->
        </div>
    </header><!-- #header -->
    <!--==========================
      Intro Section
      ============================-->
    <section id="intro">
        <div class="intro-container wow fadeIn">
            <p>Moguće nagrade</p>
            <table id="moguceNagrade">
                @{
                    List<Nagrade> nagrade = new DatabaseContext().dajNagrade();
                    foreach (Nagrade n in nagrade)
                    {
                        <tr><td><button class="listaDugmadi">@n.NazivNagrade (@n.BrojBodova poena)</button><button class="accepted">Preuzmi nagradu</button></td></tr>
                    }
                }
            </table>
            <div id="novi">
            </div>

        </div>

        <div class="formaRegister">
            <img src="~/img/redeem.png" alt="" title="">
            <p style="font-size:20px">Broj bodova: @ViewData["id"]</p>
        </div>
    </section>

    <main id="main">

        <!--==========================
          About Section
          ============================-->
        <!--==========================
          Speakers Section
          ============================-->
        <!--==========================
          Schedule Section
          ============================-->
        <!--==========================
          Venue Section
          ============================-->
        <!--==========================
          Hotels Section
          ============================-->
        <!--==========================
          Gallery Section
          ============================-->
        <!--==========================
          Sponsors Section
          ============================-->
        <section id="supporters" class="section-with-bg wow fadeInUp">

            <div class="container">
                <div class="section-header">
                    <h2>Sponzori</h2>
                </div>

                <div class="row no-gutters supporters-wrap clearfix">

                    <div class="col-lg-3 col-md-4 col-xs-6">
                        <div class="supporter-logo">
                            <img src="~/img/supporters/1.png" class="img-fluid" alt="">
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-4 col-xs-6">
                        <div class="supporter-logo">
                            <img src="~/img/supporters/2.png" class="img-fluid" alt="">
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-4 col-xs-6">
                        <div class="supporter-logo">
                            <img src="~/img/supporters/3.png" class="img-fluid" alt="">
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-4 col-xs-6">
                        <div class="supporter-logo">
                            <img src="~/img/supporters/4.png" class="img-fluid" alt="">
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-4 col-xs-6">
                        <div class="supporter-logo">
                            <img src="~/img/supporters/5.png" class="img-fluid" alt="">
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-4 col-xs-6">
                        <div class="supporter-logo">
                            <img src="~/img/supporters/6.png" class="img-fluid" alt="">
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-4 col-xs-6">
                        <div class="supporter-logo">
                            <img src="~/img/supporters/7.png" class="img-fluid" alt="">
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-4 col-xs-6">
                        <div class="supporter-logo">
                            <img src="~/img/supporters/8.png" class="img-fluid" alt="">
                        </div>
                    </div>

                </div>

            </div>

        </section>

        <!--==========================
          F.A.Q Section
          ============================-->
        <!--==========================
          Subscribe Section
          ============================-->
        <!--==========================
          Buy Ticket Section
          ============================-->
        <!--==========================
          Contact Section
          ============================-->
        <!--==========================
          Footer
          ============================-->


        <a href="#" class="back-to-top"><i class="fa fa-angle-up"></i></a>

        <!-- JavaScript Libraries -->
        <script src="~/lib/jquery/jquery.min.js"></script>
        <script src="~/lib/jquery/jquery-migrate.min.js"></script>
        <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="~/lib/easing/easing.min.js"></script>
        <script src="~/lib/superfish/hoverIntent.js"></script>
        <script src="~/lib/superfish/superfish.min.js"></script>
        <script src="~/lib/wow/wow.min.js"></script>
        <script src="~/lib/venobox/venobox.min.js"></script>
        <script src="~/lib/owlcarousel/owl.carousel.min.js"></script>


        <!-- Template Main Javascript File -->
        <script src="~/js/main.js"></script>
        <script src="~/js/skripta.js"></script>
</body>

</html>
